<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage 
	xmlns="http://xamarin.com/schemas/2014/forms" 
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
	x:Class="OneSet.AnalysisPage"
	xmlns:local="clr-namespace:OneSet;assembly=1Set"
	xmlns:i18n="clr-namespace:OneSet.Localization;assembly=1Set"
	Title="{Binding Title}">

  <TabbedPage.Children>
    <local:ChartsPage Title="{i18n:Translate ChartsTitle}"/>
    <local:ExerciseAnalysis Title="{i18n:Translate ExerciseAnalysisTitle}"/>
  </TabbedPage.Children>
</TabbedPage>